{
  "name": "react-native-thanos-snap-animation-example",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "build:android": "cd android && ./gradlew assembleDebug --no-daemon --console=plain -PreactNativeArchitectures=arm64-v8a",
    "build:ios": "cd ios && xcodebuild -workspace ThanosSnapAnimationExample.xcworkspace -scheme ThanosSnapAnimationExample -configuration Debug -sdk iphonesimulator CC=clang CPLUSPLUS=clang++ LD=clang LDPLUSPLUS=clang++ GCC_OPTIMIZATION_LEVEL=0 GCC_PRECOMPILE_PREFIX_HEADER=YES ASSETCATALOG_COMPILER_OPTIMIZATION=time DEBUG_INFORMATION_FORMAT=dwarf COMPILER_INDEX_STORE_ENABLE=NO"
  },
  "dependencies": {
    "chance": "^1.1.11",
    "node-libs-react-native": "^1.2.1",
    "pngjs": "^7.0.0",
    "react": "18.2.0",
    "react-native": "0.73.8",
    "react-native-canvas": "^0.1.40",
    "react-native-pose": "^0.9.1",
    "react-native-view-shot": "^3.8.0",
    "react-native-webview": "^13.10.5",
    "stream-browserify": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.73.21",
    "@react-native/metro-config": "0.73.5",
    "@react-native/typescript-config": "0.73.1",
    "@types/chance": "^1.1.6",
    "@types/pngjs": "^6.0.5",
    "@types/react-native-canvas": "^0.1.13",
    "babel-plugin-module-resolver": "^5.0.0"
  },
  "engines": {
    "node": ">=18"
  }
}
